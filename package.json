{
  "name": "cinema-booking-pwa-app",
  "private": true,
  "version": "0.0.1",
  "description": "Cinema Booking PWA Application",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rimraf www/* && stencil build",
    "start": "stencil build --dev --watch --serve",
    "prerender": "stencil build --prerender",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "docs": "stencil build --docs",
    "generate": "stencil generate",
    "help": "stencil -h"
  },
  "devDependencies": {
    "@ionic/core": "^5.3.2",
    "@stencil/core": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^5.4.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^5.4.1",
    "rimraf": "^3.0.2",
    "workbox-build": "^4.3.1"
  }
}
